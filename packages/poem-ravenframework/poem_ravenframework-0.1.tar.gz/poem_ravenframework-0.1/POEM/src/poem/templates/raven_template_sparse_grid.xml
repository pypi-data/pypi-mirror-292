<Simulation verbosity="debug">
  <RunInfo>
    <WorkingDir>.</WorkingDir>
    <Sequence>Run</Sequence>
    <batchSize>1</batchSize>
  </RunInfo>

  <Models>
    <EnsembleModel name="Model_Calibration" subType="">
      <!-- model list generated by the template interface -->
    </EnsembleModel>
  </Models>

  <Distributions>
    <!-- Uniform distribution generated by template interface -->
  </Distributions>

  <Samplers>
    <MonteCarlo name="MC_external">
      <samplerInit>
        <limit>10</limit>
      </samplerInit>
      <!-- variable node generated by the template interface -->
    </MonteCarlo>
  </Samplers>

  <Steps>
    <MultiRun name="Run">
      <Input   class="DataObjects"  type="PointSet"        >input_mckp</Input>
      <Model   class="Models"       type="ExternalModel"   >Model_Calibration</Model>
      <Sampler class="Samplers"     type="MonteCarlo"      >MC_external</Sampler>
      <Output  class="DataObjects"  type="PointSet"        >main_ps</Output>
      <Output  class="OutStreams"   type="Print"           >print_main</Output>
    </MultiRun>
  </Steps>

  <OutStreams>
    <Print name="print_main">
      <type>csv</type>
      <source>main_ps</source>
      <what>input,output</what>
    </Print>
  </OutStreams>

  <DataObjects>
    <!-- PointSet list generated by template interface -->
  </DataObjects>

</Simulation>
