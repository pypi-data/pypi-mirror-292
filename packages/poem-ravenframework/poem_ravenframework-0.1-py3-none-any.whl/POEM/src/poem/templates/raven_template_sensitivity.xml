<Simulation verbosity="debug">
  <RunInfo>
    <WorkingDir>sauq</WorkingDir>
    <Sequence>sample, write, stats</Sequence>
    <batchSize>1</batchSize>
  </RunInfo>

  <Models>
    <PostProcessor name="stats" subType="BasicStatistics" verbosity="debug">
      <skewness prefix="skewness">outputGroup</skewness>
      <variationCoefficient prefix="variationCoefficient">outputGroup</variationCoefficient>
      <expectedValue prefix="mean">outputGroup</expectedValue>
      <kurtosis prefix="kurtosis">outputGroup</kurtosis>
      <median prefix="median">outputGroup</median>
      <maximum prefix="max">outputGroup</maximum>
      <minimum prefix="min">outputGroup</minimum>
      <variance prefix="var">outputGroup</variance>
      <sigma prefix="sigma">outputGroup</sigma>
      <NormalizedSensitivity prefix="nsen">
        <targets>outputGroup</targets>
        <features>inputGroup</features>
      </NormalizedSensitivity>
      <sensitivity prefix="sen">
        <targets>outputGroup</targets>
        <features>inputGroup</features>
      </sensitivity>
      <pearson prefix="pearson">
        <targets>outputGroup</targets>
        <features>inputGroup</features>
      </pearson>
      <covariance prefix="cov">
        <targets>outputGroup</targets>
        <features>inputGroup</features>
      </covariance>
      <VarianceDependentSensitivity prefix="vsen">
        <targets>outputGroup</targets>
        <features>inputGroup</features>
      </VarianceDependentSensitivity>
      <spearman prefix="spearman">
        <targets>outputGroup</targets>
        <features>inputGroup</features>
      </spearman>
    </PostProcessor>
  </Models>

  <Files>

  </Files>

  <Distributions>
    <!-- Uniform distribution generated by template interface -->
  </Distributions>

  <Samplers>

  </Samplers>

  <Steps>
    <MultiRun name="sample" clearRunDir="False">
      <!-- <Input class="Files" type="">sauq</Input> -->
      <Input class="DataObjects" type="PointSet">dummyIN</Input>
      <Model class="Models" type="Code">code</Model>
      <Sampler class="Samplers" type="MonteCarlo">Sampler</Sampler>
      <Output class="DataObjects" type="PointSet">samples</Output>
    </MultiRun>
    <IOStep name="write">
      <Input class="DataObjects" type="PointSet">samples</Input>
      <Output class="OutStreams" type="Print">dump_samples</Output>
    </IOStep>
    <PostProcess name="stats">
      <Input class="DataObjects" type="PointSet">samples</Input>
      <Model class="Models" type="PostProcessor">stats</Model>
      <Output class="DataObjects" type="PointSet">stats_do</Output>
      <Output class="OutStreams" type="Print">stats_dump</Output>
    </PostProcess>
  </Steps>

  <OutStreams>
    <Print name="dump_samples">
      <type>csv</type>
      <source>samples</source>
      <what>input, output</what>
    </Print>
    <Print name="stats_dump">
      <type>csv</type>
      <source>stats_do</source>
      <what>input, output</what>
    </Print>
  </OutStreams>

  <DataObjects>
    <PointSet name="dummyIN">
      <Input>inputGroup</Input>
      <Output>OutputPlaceHolder</Output>
    </PointSet>
    <PointSet name="samples">
      <Input>inputGroup</Input>
      <Output>outputGroup</Output>
    </PointSet>
    <PointSet name="stats_do">
      <Output>
        statsGroup
      </Output>
    </PointSet>
  </DataObjects>


  <VariableGroups>

  </VariableGroups>
</Simulation>
